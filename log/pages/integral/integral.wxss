.intergral-title {
  padding: 25rpx;
  background-color: #fff;
  width: 100%;
  height: auto;
  font-size: 35rpx;
}

.go-store-btn {
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  border: 1px solid #a2a2a2;
  color: #a2a2a2;
}

.addmoney {
  color: #00549d;
}

.intergral-items {
  margin-top: 25rpx;
  padding: 0 25rpx;
  background-color: #fff;
}

.intergral-items .intergral-item {
  font-size: 35rpx;
  border-bottom: 1px solid #e2e2e2;
  padding: 25rpx 0;
  display: flex;
  flex-direction: column;
}
.flex-col{
  margin-left: 15rpx;
}
.item-wrap {
  display: flex;
  align-items: center;
}

.item-more {
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  color: #333;
}

.more-wrap {
  padding: 25rpx 0 0;
}

.intergral-items .intergral-item:last-child {
  border-bottom: 1px solid transparent;
}

.intergral-time {
  font-size: 30rpx;
  color: #a2a2a2;
}

.reduce {
  color: #e90044;
}

.add {
  color: green;
}
